XTerm*termName:                       xterm-256color
URxvt*termName:                       rxvt-unicode
URxvt*iso14755:                       false
URxvt*scrollBar:                      false
URxvt*scrollTtyKeypress:              true
URxvt*fading:                         35
URxvt*fadeColor:                      #6D878D
URxvt*cursorBlink:                    true
URxvt*font:                           xft:Meslo LG S DZ for Powerline:size=8,xft:Noto Color Emoji:size=8,xft:Symbola:pixelsize=14:antialias=true:hinting=slight
Xft.dpi:                              96
Xft.antialias:                        true
Xft.rgba:                             rgb
Xft.hinting:                          true
Xft.hintstyle:                        hintslight
Xft.lcdfilter:                        lcddefault

URxvt.perl-ext-common:                default,clipboard,url-select,keyboard-select
URxvt.url-launcher:                   /usr/bin/xdg-open
URxvt.colorUL:                        #71B5EE
URxvt.url-select.underline:           true
URxvt.keysym.M-u:                     perl:url-select:select_next
URxvt.keysym.M-Escape:                perl:keyboard-select:activate
URxvt.keysym.M-s:                     perl:keyboard-select:search

URxvt.depth:                          32

URxvt*foreground: #DDCCBB

! Grey BG
URxvt*background: #151515

!black
*color0:     #393939
*color8:     #AF875F
!red
